  <xs:element name="rFE">
    <xs:annotation>
      <xs:documentation>
        A01: Factura Electrónica (raíz)
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <!--
              GRUPO A
        -->
        <xs:element name="dVerForm" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              A02: Versión del formato
            </xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:decimal">
              <xs:totalDigits value="4"/>
              <xs:fractionDigits value="2"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="dId">
          <xs:annotation>
            <xs:documentation>
              A03: Identificador para firma electrónica
            </xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:length value="66"/>
              <xs:pattern value="[F][E](([A|V|T|E|P|N|I]|[-]|[a-zA-Z0-9]){64})?"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <!--
        
              GRUPO B
              
        -->

        <!--Datos Generales de la transacción documentada-->
        <xs:element name="gDGen" type="gDGenType" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              B01: Datos generales de la transacción documentada.
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <!--
        
              GRUPO C
              
        -->

        <!--Campos que describen cada ítem de la transacción documentada-->
        <xs:element name="gItem" type="gItemType" minOccurs="1" maxOccurs="1000">
          <xs:annotation>
            <xs:documentation>
              C01: Grupo de datos que especifica cada ítem del detalle de la transacción
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <!--
        
              GRUPO D
              
        -->

        <!--Subtotales y Totales de la Transacción-->
        <xs:element name="gTot" type="gTotType" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              D01: Grupo con información de subtotales y totales
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <!--
        
              GRUPO F
              
        -->

        <!--Campos Complementarios deUso Comercial General-->

        <!--Informaciones del Pedido Comercial Relacionadas con la Factura-->
        <xs:element name="gPedComGl" type="gPedComGlType" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              F10: Grupo de detalle de Pedido Comercial global
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <!--Informaciones de Logística-->
        <xs:element name="gInfoLog" type="gInfoLogType" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              F20: Grupo de informaciones de logística
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <!--  Identificación del Local de Entrega-->
        <xs:element name="gLcEntr" type="gLcEntrType" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              F25: Grupo de datos que identifican el local de la entrega
            </xs:documentation>
          </xs:annotation>
        </xs:element>

        <!--
        
              GRUPO ZZ
              
        -->

        <!--  Informaciones de la Firma Digital de la FE(Grupo ZZ)  -->
        <xs:element ref="ds:Signature">
          <xs:annotation>
            <xs:documentation>
              ZZ01: Firma Digital de la FE  ( grupo identificado por el atributo dId )
              Schema XML1: xmldsig-core-schema v1.00.xsd (Estándar de Firma XML).
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <!-- Final de <xs:element ref="ds:Signature">  -->

        <!--
        
              GRUPO G
              
        -->

        <!--Campos fuera de la Firma Digital de la FE-->
        <xs:element name="gNoFirm" type="gNoFirmType">
          <xs:annotation>
            <xs:documentation>
              G101: String correspondiente al QR code impreso en el CIFE
            </xs:documentation>
          </xs:annotation>
        </xs:element>

      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!--||| Definicion de Grupos |||-->
  <!-- 
  
       ************************************
  
       Grupo B
       
       ************************************
  -->

  <!--Datos Generales de la transaccion documentada-->
  <xs:complexType name="gDGenType">
    <xs:sequence>

      <!--Ambientes de destino de la FE-->
      <xs:element name="iAmb" minOccurs="1" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>
                  Producción
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>
                  Pruebas
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Tipo de Emision-->
      <xs:element name="iTpEmis" minOccurs="1" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="01">
              <xs:annotation>
                <xs:documentation>
                  Autorización de Uso Previa, Operación normal
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
              <xs:annotation>
                <xs:documentation>
                  Autorización de Uso Previa, Operación contingencia
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
              <xs:annotation>
                <xs:documentation>
                  Autorización de uso Posterior, Operación normal
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
              <xs:annotation>
                <xs:documentation>
                  Autorización de uso Posterior, Operación contingencia
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Fecha y hora de inicio de la operación en contingencia-->
      <xs:element name="dFechaCont" type="fechaTZ" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            ID: B04 Fecha y hora de inicio de la operación en contingencia
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Razón de la operación en contingencia-->
      <xs:element name="dMotCont" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="15"/>
            <xs:maxLength value="250"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Tipo de documento-->
      <xs:element name="iDoc" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            ID: B06 - Tipo de documento
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="01">
              <xs:annotation>
                <xs:documentation>Factura de Operación Interna</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
              <xs:annotation>
                <xs:documentation>Factura de Importacion</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
              <xs:annotation>
                <xs:documentation>Factura de Exportación</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
              <xs:annotation>
                <xs:documentation>Nota de Crédito referencia a una o varias FE</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="05">
              <xs:annotation>
                <xs:documentation>Nota de Débito referencia a una o varias FE</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="06">
              <xs:annotation>
                <xs:documentation>Nota de Crédito Genérica</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="07">
              <xs:annotation>
                <xs:documentation>Nota de Débito Genérica</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="08">
              <xs:annotation>
                <xs:documentation>Factura de Zona Franca</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="09">
              <xs:annotation>
                <xs:documentation>Reembolso</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Número del documento fiscal en la serie correspondiente, de 000000001 a 999999999-->
      <xs:element name="dNroDF" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B07: Número del documento fiscal en la serie correspondiente, de 000000001 a 999999999, no siendo permitido el reinicio de la numeración.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="^(?=.*[1-9].*)[0-9]{10}$"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Serie del documento fiscal, de 001 a 999-->
      <xs:element name="dPtoFacDF" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B08: Punto de Facturación del documento fiscal. La serie sirve para permitir que existan secuencias independientes de numeración de facturas, con diversas finalidades, sea por libre elección del emisor, tales como puntos de facturación distintos (como cajas de un supermercado, o bodegas de un distribuidor), tipos de productos, especies de operación, etc., sea para finalidades que vengan  a ser determinadas por la DIRECCIÓN GENERAL DE INGRESOS.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="^^(?=.*[1-9].*)[0-9]{3}$"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Código de seguridad, no se admite valor 000,000,000-->
      <xs:element name="dSeg" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B09: Codigo de seguridad.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="(?=.*[1-9].*)[0-9]{9}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Fecha de emisión del documento-->
      <xs:element name="dFechaEm" type="fechaTZ" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B10: Fecha de emisión del documento
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Fecha de salida de las mercancías-->
      <xs:element name="dFechaSalida" type="fechaTZ" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B11: Fecha de salida de las mercancías. Informar cuando sea conocida
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Naturaleza de la Operación-->
      <xs:element name="iNatOp" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B12: Naturaleza de la Operación
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="01">
              <xs:annotation>
                <xs:documentation>Venta</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
              <xs:annotation>
                <xs:documentation>Exportación</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="10">
              <xs:annotation>
                <xs:documentation>Transferencia</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="11">
              <xs:annotation>
                <xs:documentation>Devolución</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="12">
              <xs:annotation>
                <xs:documentation>Consignación</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="13">
              <xs:annotation>
                <xs:documentation>Remesa</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="14">
              <xs:annotation>
                <xs:documentation>Entrega gratuita</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="20">
              <xs:annotation>
                <xs:documentation>Compra</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="21">
              <xs:annotation>
                <xs:documentation>Importación</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Tipo de la operación-->
      <xs:element name="iTipoOp" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B13: Tipo de la operación
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Salida o Venta</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Entrada o Compra</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Destino u origen de la operacion-->
      <xs:element name="iDest" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B14: Destino u origen de la operación
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Panamá</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Extranjero</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Formato de generación del CAFE-->
      <xs:element name="iFormCAFE" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B15: Formato de generación del CAFE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Sin generación de CAFE</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Cinta de papel</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>Papel formato carta</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Manera de entrega del CIFE al receptor-->
      <xs:element name="iEntCAFE" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B16: Manera de entrea del CAFE al receptor
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Sin generación de CAFE</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>CAFE entregado para el receptor en papel</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>CAFE enviado para el receptor en formato electrónico</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Envío del contenedor para el receptor-->
      <xs:element name="dEnvFE" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B17: Envío del contenedor para el receptor
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Normal</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>El receptor exceptúa  al emisor de la obligatoriedad de envío del contenedo</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Proceso de generación de la FE-->
      <xs:element name="iProGen" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B18: Proceso de generación de la FE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Generación por el sistema de facturación del contribuyente</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Generación por tercero contratado</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>Generación gratuita por tercero proveedor de solución</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
              <xs:annotation>
                <xs:documentation>Generación gratuita por la DGI en página web</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <!--  RCN - comentado por petición de Yarelis, opción que se habilitará en el futuro.
            <xs:enumeration value="5">
              <xs:annotation>
                <xs:documentation>Generación gratuita por la DGI en aplicativo móvil</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
-->
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Tipo de transacción de venta-->
      <xs:element name="iTipoTranVenta" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B19: Tipo de transacción de venta
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Ventas del Giro</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Venta Activo Fijo</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>Venta Bien Raíz</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
              <xs:annotation>
                <xs:documentation>Prestación de Servicio</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Tipo de Sucursal-->
      <xs:element name="iTipoSuc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B20: Tipo de Sucursal
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Mayor cantidad de Operaciones venta al detal (retail)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Mayor cantidad de Operaciones venta al por mayor</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Informaciones de interés del emitente con respeto a la FE-->
      <xs:element name="dInfEmFE" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B29: Informaciones de interés del emitente con respecto a la FE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="5000"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Grupo de datos que identifican al emisor-->
      <xs:element name="gEmis" type="gEmisType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B30: Grupo de datos que identifican al emisor
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Grupo de datos que identifican al receptor-->
      <xs:element name="gDatRec" type="gDatRecType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B40: Grupo de datos que identifican al receptor
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Grupo de datos de factura en caso de exportación-->
      <xs:element name="gFExp" type="gFExpType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B50: Grupo de datos de facturas en caso de exportación
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Informacipon de documento fiscal referenciado-->
      <xs:element name="gDFRef" type="gDFRefType" minOccurs="0" maxOccurs="99">
        <xs:annotation>
          <xs:documentation>
            B60: Información de documento fiscal referenciado
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Grupo de datos que identifican al autorizado a descargar-->
      <xs:element name="gAutXML" minOccurs="0" maxOccurs="10">
        <xs:annotation>
          <xs:documentation>
            B70: Grupo de datos que identifican al autorizado a descargar
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <!--RUC del autorizado a descargar-->
            <xs:element name="gRucAutXML" type="RucType" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  B701: RUC del autorizado a descargar
                </xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <!--Grupo de datos que identifican al emisor-->
  <xs:complexType name="gEmisType">
    <xs:sequence>

      <!--Ruc del contribuyente emisor-->
      <xs:element name="gRucEmi" type="RucType" minOccurs="1" maxOccurs="1">
        <!-- FICTEC: cambiar codigo y estructura en la ficha técnica-->
        <xs:annotation>
          <xs:documentation>
            B301: Tipo, RUC y DV del Contribuyente Emisor
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Razon Social o Nombre y Apellido del emisor de la FE-->
      <xs:element name="dNombEm" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B302: Razón Social (persona jurídica) o Nombre y Apellido (persona natural) del emisor de la FE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Código de la sucursal desde donde se emite la factura-->
      <xs:element name="dSucEm" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B303: Código de la sucursal desde donde se emite la factura
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9]{4}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Coordinadas geográficas de la sucursal donde se ubica el punto de facturación-->
      <xs:element name="dCoordEm" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B304: Coordenadas geográficas de la sucursal donde se ubica el punto de facturación
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">

            <xs:maxLength value="22"/>
            <!--ej: 8.974499,-79.5340337 -->


            <xs:pattern value="^([-+]?)([\d]{1,2})(((\.)[\d]{4,6}(,)))(-([\d]{2})(\.)[\d]{4,6})$"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Dirección de la sucursal emisora, o de la persona física emisora-->
      <xs:element name="dDirecEm" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B305: Dirección de la sucursal emisora, o de la persona física emisora
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!-- Ubicación exacta del Emisor -->
      <xs:element name="gUbiEm" type="cCodUbicType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B306: Codigo, Corregimiento, Distrito, Provincia donde se ubica el punto de facturación
          </xs:documentation>
        </xs:annotation>
      </xs:element>


      <!--Teléfono de contacto de la sucursal emisora o de la persona emisora-->
      <xs:element name="dTfnEm" type="telefonoType" minOccurs="1" maxOccurs="3">
        <xs:annotation>
          <xs:documentation>
            B309: Teléfono de contacto de la sucursal emisora o de la persona emisora
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Correo Electrónico del emisor-->
      <xs:element name="dCorElectEmi" type="emailType" minOccurs="0" maxOccurs="3">
        <xs:annotation>
          <xs:documentation>
            B310: Correo electrónico del emisor
          </xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <!--Grupo de datos que identifican al receptor-->
  <xs:complexType name="gDatRecType">
    <xs:sequence>

      <!--Tipo de Receptor, si es consumidor final o contribuyente que puede usar crédito fiscal-->
      <xs:element name="iTipoRec" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            ID: B401 - Identifica el tipo de receptor de la FE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="01">
              <xs:annotation>
                <xs:documentation>
                  Contribuyente
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
              <xs:annotation>
                <xs:documentation>
                  Consumidor final
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
              <xs:annotation>
                <xs:documentation>
                  Gobierno
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
              <xs:annotation>
                <xs:documentation>
                  Extranjero
                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Ruc del Receptor-->
      <xs:element name="gRucRec" type="RucRecType" minOccurs="0" maxOccurs="1">
        <!-- FICTEC: cambiar codigo y estructura en la ficha tecnica-->
        <xs:annotation>
          <xs:documentation>
            402: RUC del Contribuyente Receptor
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Razon Social o Nombre y apellido del Receptor-->
      <xs:element name="dNombRec" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B403: Razón social (persona jurídica) o Nombre y Apellido (persona natural) del receptor de la FE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Dirección del receptor de la FE-->
      <xs:element name="dDirecRec" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B404: Dirección del receptor de la FE
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Ubicacion del receptor-->
      <xs:element name="gUbiRec" type="cCodUbicType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B405: Codigo, Corregimiento, Distrito, Provincia donde se ubica el punto de facturación
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Identificación de extranjeros -->
      <xs:element name="gIdExt" type="gIdExtType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B406: Identificación de extranjeros
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Teléfono de contacto del receptor de la FE-->
      <xs:element name="dTfnRec" type="telefonoType" minOccurs="0" maxOccurs="3">
        <xs:annotation>
          <xs:documentation>
            B408: Teléfono de contacto del receptor de la FE
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Correo Electrónico del Receptor-->
      <xs:element name="dCorElectRec" type="emailType" minOccurs="0" maxOccurs="3">
        <xs:annotation>
          <xs:documentation>
            B409: Correo electrónico del receptor
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--País del receptor de la FE-->
      <xs:element name="cPaisRec" type="paisType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation> B411: País del receptor de la FE. Debe ser PAN(Panamá) si B15=1 (destino u origen de la operacion es Panamá)</xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--País del receptor de la FE no existente en la Tabla 20-->
      <xs:element name="dPaisRecDesc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B411: País del receptor de la FE no existente en la tabla
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="5"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <!--Grupo de datos de factura en caso de exportación-->
  <xs:complexType name="gFExpType">
    <xs:sequence>

      <!--Condiciones de entrega de acuerdo con la tabla de INCOTERMS-->
      <xs:element name="cCondEntr" type="incotermsType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B501: Condiciones de entrega de acuerdo con la tabla de INCOTERMS
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Moneda de la operación de exportación: moneda en que se vende al comprador en el extranjero-->
      <xs:element name="cMoneda" type="monedasType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B502: Moneda de la operación de exportación: moneda en que se vende al comprador en el extranjero
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Moneda de la operación de exportación no existente en la tabla 22-->
      <xs:element name="cMonedaDesc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B503: Moneda de la operación de exportación no existe en la tabla monedas
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Tipo de cambio a la fecha de la operacion-->
      <xs:element name="dCambio" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B504: Tipo de cambio a la fecha de la operación
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:pattern value="[0-9]{1,11}.[0-9]{2,4}"/>
            <xs:totalDigits value="11"/>
            <xs:fractionDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Monto en la moneda extranjera-->
      <xs:element name="dVTotEst" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B505: Monto en la moneda extranjera
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:totalDigits value="20"/>
            <xs:fractionDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Puertode embarque de la mercancía-->
      <xs:element name="dPuertoEmbarq" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B506: Puerto de embarque de la mercacía
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="5"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <!--Informacipon de documento fiscal referenciado-->
  <xs:complexType name="gDFRefType">
    <xs:sequence>

      <xs:element name="gRucEmDFRef" type="RucType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B601: RUC del emisor del documento fiscal referenciado
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--Razon social o Nombre y Apellido del emisor del documento fiscal referenciado-->
      <xs:element name="dNombEmRef" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B602: Razón Social (Persona Jurídica) o Nombre y Apellido (Persona Natural) del emisor del documento fiscal referenciado
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--Fecha de emisión del documento fiscal referenciado-->
      <xs:element name="dFechaDFRef" type="fechaTZ" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B603: Fecha de emisión del Documento Fiscal Referenciado
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="gDFRefNum" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            B604: Información de Referencia de la FE
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>

            <!--Infomracion de Referencia a FE-->
            <xs:element name="gDFRefFE" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  B605: Infomracion de Referencia de la FE
                </xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>

                  <xs:element name="dCUFERef" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                      <xs:documentation>
                        B606: CUFE de FE referenciada
                      </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:length value="66"/>
                        <xs:pattern value="[F][E](([A|V|T|E|P|N|I]|[-]|[a-zA-Z0-9]){64})?"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>

                </xs:sequence>
              </xs:complexType>
            </xs:element>

            <!--Información de Referencia a factura en papel-->
            <xs:element name="gDFRefFacPap" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  B615: Información de Referencia a factura en papel
                </xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="dNroFacPap" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                      <xs:documentation>
                        B616: Número de factura en papel
                      </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="22"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>

            <!--Información de referenia a factura emitida-->
            <xs:element name="gDFRefFacIE" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  B620: Infomración de Referencia a factura en papel
                </xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="dNroFacIE" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                      <xs:documentation>
                        B621: Número de factura emitida por impresora fiscal
                      </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="22"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>

          </xs:choice>
        </xs:complexType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <!-- 
  
       ************************************
  
       Grupo C
       
       ************************************
  -->

  <!--Campos que describen cada ítem de la transacción documentada-->
  <xs:complexType name="gItemType">
    <xs:annotation>
      <xs:documentation>
        Definición de tipo para el grupo:
        C01: Grupo de Campos Fuera de la Firma Digital de la FE
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element name="dSecItem" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C02:Número secuencial del ítem</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:totalDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dDescProd" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C03:Descripción del producto o servicio</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="500"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dCodProd" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C04:Código interno del Ítem</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="cUnidad" type="unidadesMedida" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C05:Unidad de medida del código interno</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dCantCodInt" type="montoTipo31" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C06:Cantidad del producto o servicio en la unidad de medida del código interno</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dFechaFab" type="fechaYMD" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C07:Fecha de fabricación/elaboración</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dFechaCad" type="fechaYMD" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C08:Fecha de caducidad</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dCodCPBSabr" type="catBienes" minOccurs="0" maxOccurs="1">
        <!--FICTEC agregar tabla de bienes y servicios de panamá-->
        <xs:annotation>
          <xs:documentation>C09:Código del Ítem en la Codificación Panameña de Bienes y Servicios Abreviada</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dCodCPBScmp" type="descBienes" minOccurs="0" maxOccurs="1">
        <!--FICTEC agregar tabla de bienes y servicios de panamá-->
        <xs:annotation>
          <xs:documentation>C10:Código del Ítem en la Codificación Panameña de Bienes y Servicios</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="cUnidadCPBS" type="unidadesMedida" minOccurs="0" maxOccurs="1">
        <!--FICTEC verificar tabla con unidad de medida de bienes y servicios-->
        <xs:annotation>
          <xs:documentation>C11:Unidad de medida en la Codificación Panameña de Bienes y Servicios</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dInfEmFE" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C19:Informaciones de interés del emitente con respeto a un ítem de la FE</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="5000"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--- 6.5.1. Grupo de Precios del Ítem -->
      <xs:element name="gPrecios" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C20:Grupo de precios del ítem</xs:documentation>
        </xs:annotation>

        <xs:complexType>
          <xs:sequence>

            <xs:element name="dPrUnit" type="montoTipo06" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C201:Precio unitario de la transferencia del bien o servicio</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dPrUnitDesc" type="montoTipo06" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C202:Descuento en el precio del ítem</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dPrItem" type="montoTipo06" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C203:Precio del ítem</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dPrAcarItem" type="montoTipo02" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C204:Precio del acarreo, para este ítem específico</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dPrSegItem" type="montoTipo02" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C205:Precio del seguro, para este ítem específico</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dValTotItem" type="montoTipo06" minOccurs="1" maxOccurs="1">

              <xs:annotation>
                <xs:documentation>C206:Suma del precio del ítem con los montos de los impuestos</xs:documentation>
              </xs:annotation>

            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <!--- 6.5.2. Grupo de Otros Códigos del Ítem -->
      <xs:element name="gCodItem" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>

            <xs:element name="dGTINCom" type="gGtinType" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C301:Código GTIN del ítem, para la unidad de comercialización</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dCantGTINCom" type="montoTipo31" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C302:Cantidad del producto o servicio en el Código GTIN del ítem de comercialización</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dGTINInv" type="gGtinType" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C303:Código GTIN del ítem, para la unidad de inventario</xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dCantComInvent" type="montoTipo31" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C304:Cantidad del producto o servicio en el Código GTIN del ítem de inventario</xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <!--- 6.5.3. Grupo de ITBMS del Ítem -->
      <xs:element name="gITBMSItem" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C40:Grupo de ITBMS del ítem</xs:documentation>
        </xs:annotation>

        <xs:complexType>
          <xs:sequence>

            <xs:element name="dTasaITBMS" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C401:Tasa del ITBMS aplicable al ítem</xs:documentation>
              </xs:annotation>

              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="00">
                    <xs:annotation>
                      <xs:documentation>
                        Tasa 0 (Exonerado)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="01">
                    <xs:annotation>
                      <xs:documentation>
                        Tasa 7% (Regular)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="02">
                    <xs:annotation>
                      <xs:documentation>
                        Tasa 10% (alcoholes y derivados)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="03">
                    <xs:annotation>
                      <xs:documentation>
                        Tasa 15% (cigarrillos)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dValITBMS" type="montoTipo06" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C402:Monto del ITBMS del ítem</xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <!--- 6.5.4. Grupo de ISC del Ítem -->
      <xs:element name="gISCItem" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>C50:Grupo de ISC del ítem</xs:documentation>
        </xs:annotation>

        <xs:complexType>
          <xs:sequence>

            <xs:element name="dTasaISC" minOccurs="0" maxOccurs="1">
              <!-- FICTEC consultar los impuestos ISC-->
              <xs:annotation>
                <xs:documentation>C501:Tasa del ISC aplicable al ítem</xs:documentation>
              </xs:annotation>

              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:enumeration value="0.05">
                    <xs:annotation>
                      <xs:documentation>
                        Impuesto Selectivo al Consumo de Gaseosas
                        Impuesto Selectivo al Consumo de Joyas y Armas de Fuego
                        Impuesto Selectivo al Consumo de televisión por cable, microondas y satelital, telefonía móvil
                        Impuesto Selectivo al Consumo de Bebidas Alcohólicas (B/. 0.05 por litro según detalle)
                        Impuesto Selectivo al Consumo de Vinos (B/. 0.05 por litro según detalle)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                  <xs:enumeration value="0.06">
                    <xs:annotation>
                      <xs:documentation>
                        Jarabes, siropes o concentrados
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                  <xs:enumeration value="0.00">
                    <xs:annotation>
                      <xs:documentation>
                        Impuesto Selectivo al Consumo de Bebidas Espirituosas
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                  <xs:enumeration value="0.1325">
                    <xs:annotation>
                      <xs:documentation>
                        Impuesto Selectivo al Consumo de Cervezas (B/. 0.1325 por litro según detalle)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                  <xs:enumeration value="0.325">
                    <xs:annotation>
                      <xs:documentation>
                        Impuesto Selectivo al Consumo de Cigarrillos y tabacos
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                  <xs:enumeration value="0.035">
                    <xs:annotation>
                      <xs:documentation>
                        Impuesto Selectivo al Consumo de Licores
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                  <xs:enumeration value="0.00">
                    <xs:annotation>
                      <xs:documentation>
                        Impuesto Selectivo al Consumo de Minería no metálica
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>

                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dValISC" type="montoTipo06" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C502:Monto del ISC del ítem</xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>





      <!--- Grupo de Otras Tasas o Impuestos (OTI) del Item -->
      <xs:element name="gOTIItem" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>C60:Grupo de Otras Tasas o Impuestos (OTI) del ítem</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="dCodOTI" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C601:Código de Otras Tasas o Impuesto del ítem</xs:documentation>
              </xs:annotation>

              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="01">
                    <xs:annotation>
                      <xs:documentation>
                        SUMA911
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="02">
                    <xs:annotation>
                      <xs:documentation>
                        Tasa Portabilidad Numérica
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dValOTI" type="montoTipo06" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>C602:Monto de Otras Tasas o Impuesto del ítem</xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>

      </xs:element>


      <!--
        
              GRUPO E
              
        -->

      <!-- 6.7.1 Grupo E - Vehículos Nuevos-->
      <xs:element name="gVehicNuevo" type="gVehicNuevoType" minOccurs="0" maxOccurs="1">
        <!--FICTEC: cambiar en la ficha técnica-->
        <xs:annotation>
          <xs:documentation>
            E05: Grupo de detalle de vehículos nuevos
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <!-- 6.7.2 Grupo E - Medicinas y Materias Primas Farmacéuticas-->
      <xs:element name="gMedicina" type="gMedicinaType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            E10: Grupo de detalle de medicinas y materias primas farmacéuticas
          </xs:documentation>
        </xs:annotation>
      </xs:element>



      <!--Informaciones del Pedido Comercial Relacionadas Con Un Ítem de la Factura-->
      <xs:element name="gPedComIr" type="gPedComIrType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            E15: Grupo de detalle de Pedido Comercial de ítem
          </xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>
  </xs:complexType>


  <!--
  
        *************************************
  
            Grupo D
      
        *************************************
  
  -->

  <!--Subtotales y Totales de la Transacción-->
  <xs:complexType name="gTotType">
    <xs:annotation>
      <xs:documentation>
        Definición de tipo para el grupo:
        D01: Grupo con información de subtotales y totales
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element name="dTotNeto" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D02: Suma de los precios antes de impuesto
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotITBMS" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D03: Total del ITBMS
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotISC" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D04: Total del ISC
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotGravado" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D05: Suma total de monto gravado
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotDesc" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D06: Suma de los descuentos y bonificaciones concedidos sobre el valor total de la factura
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotAcar" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D07: Valor del acarreo cobrado en el precio total
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotSeg" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D08: Valor del seguro cobrado en el precio total
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dVTot" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D09: Valor total de la factura
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotRec" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D10: Suma de los valores recibidos
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dVuelto" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D11: Vuelto entregado al cliente
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="iPzPag" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D12: Tiempo de pago
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Inmediato</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Plazo</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>Mixto</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dNroItems" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D13: Número total de ítems de la factura
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="11"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dVTotItems" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D14: Suma total de los ítems con los montos de los impuestos
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="gDescBonif" minOccurs="0" maxOccurs="5">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D20: Grupo de datos de que describen descuentos o bonificaciones adicionales aplicados a la factura
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="dDetalDesc">
              <xs:annotation>
                <xs:documentation>
                  D200: Descripción de descuentos o bonificaciones adicionales aplicados a la factura
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="500"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dValDesc" type="montoTipo01">
              <xs:annotation>
                <xs:documentation>
                  D201: Monto Descuentos/Bonificaciones y otros ajustes
                </xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Final de <xs:element name="gDescBonif">  -->

      <xs:element name="gFormaPago" minOccurs="1" maxOccurs="10">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D30: Grupo de formas de pago de la factura
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="iFormaPago" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D301: Forma de pago de la factura
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:enumeration value="01">
                    <xs:annotation>
                      <xs:documentation>Crédito</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="02">
                    <xs:annotation>
                      <xs:documentation>Contado</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="03">
                    <xs:annotation>
                      <xs:documentation>Tarjeta Crédito</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="04">
                    <xs:annotation>
                      <xs:documentation>Tarjeta Débito</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="05">
                    <xs:annotation>
                      <xs:documentation>Tarjeta Fidelidad</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="06">
                    <xs:annotation>
                      <xs:documentation>Vale devolución</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="07">
                    <xs:annotation>
                      <xs:documentation>Vale regalo</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
          <xs:enumeration value="08">
                    <xs:annotation>
                      <xs:documentation>Transf. / Depósito a cta. bancaria</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="99">
                    <xs:annotation>
                      <xs:documentation>Otro</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dFormaPagoDesc" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D302: Descripción de forma de pago no listada en el formato
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:annotation>
                    <xs:documentation>Obligatorio si existe D301(iFormaPago)</xs:documentation>
                  </xs:annotation>
                  <xs:minLength value="10"/>
                  <xs:maxLength value="100"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dVlrCuota" type="montoTipo01" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D303: Valor de la fracción pagada utilizando esta forma de pago
                </xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Final de <xs:element name="gFormaPago">  -->

      <xs:element name="gRetenc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D40: Grupo datos cuando a la factura aplican retenciones
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="cCodRetenc" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D401: Código de Retención a aplicar
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="2"/>
                  <xs:enumeration value="1">
                    <xs:annotation>
                      <xs:documentation>Pago por servicio profesional al estado 100% 1</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="2">
                    <xs:annotation>
                      <xs:documentation>Pago por venta de bienes/servicios al estado 50%</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="3">
                    <xs:annotation>
                      <xs:documentation>Pago o acreditación a no domiciliado o empresa constituida en el exterior 100%</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
